GEMINI_API_KEY=your_gemini_api_key_here
GITHUB_TOKEN=your_github_token_here
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/ai-codebase-recommender

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-super-secret-key-change-this-in-production

# Google OAuth (Get from: https://console.cloud.google.com/apis/credentials)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# GitHub OAuth (Get from: https://github.com/settings/developers)
GITHUB_CLIENT_ID=your_github_client_id_here
GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Pinecone (Get from: https://www.pinecone.io/)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1-aws

# Vercel (Optional - for programmatic deployments)
VERCEL_TOKEN=your_vercel_token_here

# Admin Access (Only these emails can access /admin - comma separated)
ADMIN_EMAILS=your_email@gmail.com,another_admin@gmail.com
NEXT_PUBLIC_ADMIN_EMAILS=your_email@gmail.com,another_admin@gmail.com

